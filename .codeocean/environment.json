{
	"version": 1,
	"base_image": "codeocean/mambaforge3:23.1.0-4-python3.10.12-ubuntu22.04",
	"args": [
		"API_KEY",
		"API_SECRET",
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"pip3": {
			"packages": [
				{
					"name": "aind-session",
					"version": "0.3.12"
				},
				{
					"name": "pandas",
					"version": "2.2.3"
				},
				{
					"name": "panel",
					"version": "1.5.4"
				},
				{
					"name": "streamlit",
					"version": "1.40.2"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}